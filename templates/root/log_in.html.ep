<!doctype html>
<html lang="en">
<head>
    <title>PDFUnicorn - Log In</title>
	%= include 'site/head'
	%= include 'mixpanel'
  	<script type="text/javascript">mixpanel.track('view-login-page')</script>
</head>

<body>
	%= include 'site/header'
	<div class="container">
		% if ($error){
			<p class="text-danger lead"><%= $error %></p>
		% }
		% if ($message){
			<p class="text-success lead"><%= $message %></p>
		% }
		%= include 'site/login_form'
	</div>
</body>
</html>
